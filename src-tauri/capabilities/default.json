{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"core:image:default",
		"fs:default",
		"fs:allow-read-dir",
		"fs:allow-read-file",
		"fs:allow-write-file",
		"fs:allow-stat",
		"fs:allow-exists",
		"fs:scope-home-recursive",
		"dialog:default",
		"sql:default",
		"sql:allow-load",
		"sql:allow-execute",
		"sql:allow-select",
		"os:default",
		"process:default",
		"process:allow-restart"
	]
}
